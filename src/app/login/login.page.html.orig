    <div class="ion-padding-horizontal" *ngIf="brandId == 146 || brandId == 148">
      <ion-button style="height: 50px" expand="block" class="ion-no-margin" (click)="register()">Cadastrar-se</ion-button>
      <ion-label style="padding: 0.25rem; font-size: 15px; font-weight: bold;text-align: center;" class="ion-text-wrap ion-text-center" (click)="register()">Não tem uma conta? Não tem problema! Crie sua conta rapidamente para finalizar seu pedido.</ion-label>
    </div>
    <ion-item button lines="none" style="max-width: 800px; margin: auto;">
      <ion-label class="ion-text-wrap ion-text-center" (click)="forgotPassword()"><u><b style="color: var(--ion-color-primary)">Esqueci minha senha</b></u></ion-label>
    </ion-item>
    <ion-item button lines="none" style="max-width: 800px; margin: auto;" *ngIf="brandId != 146 && brandId != 148">
      <ion-label class="ion-text-wrap ion-text-center" (click)="register()">Primeira vez aqui? <u><b style="color: var(--ion-color-primary)" >Clique aqui para se cadastrar.</b></u></ion-label>
    </ion-item>
