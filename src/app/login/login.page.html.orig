        <ion-button expand="block" (click)="facebookNotEnabled()" style="height: 50px; --background: #3b5998;" *ngIf="!facebookLoginEnabled"><ion-icon name="logo-facebook" style="margin-right: 5px;"></ion-icon>Entrar com o Facebook</ion-button>
